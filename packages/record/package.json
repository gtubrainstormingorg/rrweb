{
  "name": "howdygo-rrweb-record",
  "version": "2.0.0-alpha.30",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "rrweb",
    "@rrweb/record"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "yarn turbo run prepublish",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "check-types": "tsc -noEmit",
    "prepublish": "tsc -noEmit && vite build",
    "lint": "yarn eslint src/**/*.ts"
  },
  "homepage": "https://github.com/rrweb-io/rrweb/tree/main/packages/@rrweb/record#readme",
  "bugs": {
    "url": "https://github.com/rrweb-io/rrweb/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rrweb-io/rrweb.git"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/howdygo-rrweb-record.cjs",
  "module": "./dist/howdygo-rrweb-record.js",
  "unpkg": "./dist/howdygo-rrweb-record.umd.cjs",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/howdygo-rrweb-record.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/howdygo-rrweb-record.cjs"
      }
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "devDependencies": {
    "puppeteer": "^20.9.0",
    "vite": "^5.3.1",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.4.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "howdygo-rrweb-types": "^2.0.0-alpha.30",
    "howdygo-rrweb": "^2.0.0-alpha.30"
  },
  "browserslist": [
    "supports es6-class"
  ]
}
